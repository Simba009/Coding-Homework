a_list = [2,3,4,6,8,10] 
print(a_list[1]) this prints out 3 
a_dict = dict([[0,"zero"],[0.5,"half"],[1,"one"]])
print(0 in a_dict) prints out zero
print(0 not in a_dict) false
print(2 in a_dict) error
print(2 not in a_dict) true
print(a_dict[0]) zero
print(a_dict[1]) one
print(a_dict.get(2)) none
print(a_dict[2]) error
